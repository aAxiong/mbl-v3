.search {
  background: #f8f8f8;
  padding: 15rpx 30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.search .search-box {
  width: 625rpx;
  height: 75rpx;
  background: #e3e3e3;
  border-radius: 100rpx;
  padding-left: 35rpx;
  line-height: 75rpx;
  display: flex;
  align-items: center;
}

.search .search-box.active {
  width: 570rpx;
  margin-right: 20rpx;
}

.search-text {
  color: #999;
  font-size: 30rpx;
}

.search .search-box image {
  width: 46rpx;
  height: 46rpx;
}

.phcolor {
  color: #fff;
}

.search .search-box input {
  width: 520rpx;
  color: #fff;
  line-height: 75rpx;
  font-size: 30rpx;
  margin-left: 20rpx;
  height: 75rpx;
}

.total_page {
  position: relative;
}

.total_page .search_page {
  width: 100%;
  height: 80vh;
  padding: 0 0 140rpx 0;
  background: #fff;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 52;
}

.total_page .search_page.active {
  z-index: 49;
}

.total_page .search_page .labelBox {
  padding: 35rpx 10rpx;
}

.total_page .search_page .labelBox .tit {
  color: #333;
  font-size: 32rpx;
  margin-bottom: 5rpx;
  margin-left: 10rpx;
}

.total_page .search_page .labelBox .content .label {
  padding: 15rpx 20rpx;
  background: #f7f7f7;
  font-size: 28rpx;
  color: #808080;
  margin: 10rpx;
  display: inline-block;
  border-radius: 5rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  max-width: 135rpx;
}

.total_page .search_page .labelBox .label_hisBox {
  /* margin-top: 100rpx; */
}

.total_page .search_page .labelBox .label_hisBox text {
  float: right;
  color: #ccc;
  font-size: 28rpx;
}

.goods {
  display: flex;
  width: 100%;
  overflow: hidden;
  font-family: "微软雅黑";
}

.goods .menu-wrapper {
  flex: 0 0 175rpx;
  width: 175rpx;
  background: #f8f8f8;
}

.goods .menu-wrapper .menu-item {
  display: table;
  width: 146rpx;
  height: 115rpx;
  line-height: 20px;
  /* padding: 0 15rpx; *//* border-left: 4rpx solid #f8f8f8; */
}

.goods .menu-wrapper .menu-item.active {
  position: relative;
  z-index: 10;
  /* background: #fff; *//* border-left: 4rpx solid #fd2e2e; */
}

.goods .menu-wrapper .menu-item:current {
  position: relative;
  z-index: 10;
  margin-top: -1px;
  background: #fff;
  font-weight: 700;
}

.goods .menu-wrapper .menu-item:current .text {
  border: none;
}

.goods .menu-wrapper .menu-item .icon {
  display: inline-block;
  width: 12px;
  height: 12px;
  vertical-align: top;
  margin-right: 4px;
  background-size: 12px 12px;
  background-repeat: no-repeat;
}

.goods .menu-wrapper .menu-item .text {
  display: table-cell;
  width: 56px;
  vertical-align: middle;
  position: relative;
  font-size: 32rpx;
  border-bottom: 1px solid rgba(7, 17, 27, 0.1);
  color: #333;
  z-index: 3;
  padding: 0 15rpx;
}

.goods .menu-wrapper .menu-item.active .text {
  /* border-bottom: 1px solid #fff; */
}

.goods .menu-wrapper .menu-item.active .activeImg {
  position: absolute;
  left: 0;
  top: -17rpx;
  width: 172rpx;
  height: 150rpx;
}

.goods .menu-wrapper .menu-item .text image.hoticon {
  width: 25rpx;
  height: 32rpx;
}

.goods .foods-wrapper {
  flex: 1;
}

.goods .foods-wrapper {
  height: 486.95px;
}

.goods .foods-wrapper .food-list .title {
  height: 20rpx;
}

.goods .foods-wrapper .food-list .food-item {
  display: flex;
  margin: 20rpx;
  padding-bottom: 32rpx;
  position: relative;
  border-bottom: 2rpx solid #f5f5f5;
}

.goods .foods-wrapper .food-list .food-item:first-child {
  margin-top: 0px;
}

.goods .foods-wrapper .food-list .food-item .icon {
  flex: 0 0 205rpx;
  margin-right: 10px;
}

.goods .foods-wrapper .food-list .food-item .icon image {
  border-radius: 10rpx;
}

.goods .foods-wrapper .food-list .food-item .content {
  flex: 1;
}

.goods .foods-wrapper .food-list .food-item .content .name {
  margin-bottom: 8rpx;
  font-size: 32rpx;
  color: #333;
}

.goods .foods-wrapper .food-list .food-item .content .desc, .extra {
  font-size: 22rpx;
  line-height: 10px;
  color: #ccc;
}

.goods .foods-wrapper .food-list .food-item .content .desc {
  margin-bottom: 8px;
  line-height: 12px;
}

.goods .foods-wrapper .food-list .food-item .content .extra .count {
  margin-right: 12px;
  font-size: 22rpx;
}

.goods .foods-wrapper .food-list .food-item .content .price {
  font-weight: 700px;
  line-height: 30rpx;
  /* height: 30rpx; */
}

.goods .foods-wrapper .food-list .food-item .content .price .now {
  margin-right: 8px;
  font-size: 38rpx;
  color: #fd2e2e;
  margin-top: 10rpx;
}

.goods .foods-wrapper .food-list .food-item .content .price .now text {
  float: right;
  font-size: 22rpx;
  color: #666;
}

.goods .foods-wrapper .food-list .food-item .content .price .old {
  line-height: 40rpx;
  height: 40rpx;
  font-size: 22rpx;
  color: #fd2e2e;
  margin-top: 3rpx;
}

.goods .foods-wrapper .food-list .food-item .content .price .old image {
  width: 46rpx;
  height: 28rpx;
  vertical-align: middle;
  margin-right: 5rpx;
}

.goods .foods-wrapper .food-list .food-item .content .cartControl-wrapper {
  position: absolute;
  right: 0;
  bottom: 80rpx;
}

/* cartControl 样式 */

.goods .foods-wrapper .food-list .food-item .content .cartControl-wrapper .cartControl {
  /* font-size: 0; */
}

.food-item .cart-add {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.food-item .cart-add .text {
  width: 60rpx;
  text-align: center;
  font-size: 28rpx;
}

.goods .foods-wrapper .food-list .food-item .content .cartControl-wrapper .cartControl .cart-decrease,
.cart-add {
  display: inline-block;
  padding: 10rpx;
}

.cart-add image {
  width: 44rpx;
  height: 44rpx;
}

.cart-decrease image {
  width: 44rpx;
  height: 44rpx;
}

.goods .foods-wrapper .food-list .food-item .content .cartControl-wrapper .cartControl .cart-decrease,
.cart-add :fade-enter-active, :fade-leave-active {
  transition: all 0.4s linear;
}

.goods .foods-wrapper .food-list .food-item .content .cartControl-wrapper .cartControl .cart-decrease,
.cart-add :fade-enter, :fade-leave-active {
  opacity: 0 transform translate3d(24px, 0, 0);
}

.goods .foods-wrapper .food-list .food-item .content .cartControl-wrapper .cartControl .cart-decrease,
.cart-add .inner {
  display: inline-block;
  line-height: 24px;
  font-size: 24px;
  vertical-align: top;
  color: rgb(0, 160, 220, 0.2);
}

.goods .foods-wrapper .food-list .food-item .content .cartControl-wrapper .cartControl .cart-decrease,
.cart-add :inner-enter-active, :inner-leave-active {
  transition: all 0.4s linear;
  transform: rotate(0);
}

.goods .foods-wrapper .food-list .food-item .content .cartControl-wrapper .cartControl .cart-decrease,
.cart-add :inner-enter, :inner-leave-active {
  opacity: 0 transform rotate(180deg);
}

.goods .foods-wrapper .food-list .food-item .content .cartControl-wrapper .cartControl .cart-count {
  display: inline-block;
  font-size: 10px;
  line-height: 24px;
  width: 12px;
  vertical-align: top;
  padding-top: 6px;
  text-align: center;
  color: rgb(147, 153, 159);
}

/* shopcart 样式 */

.shopCart {
  position: fixed;
  left: 0px;
  bottom: 0px;
  z-index: 50;
  width: 100%;
  height: 96rpx;
}

.shopCart .content {
  display: flex;
  background: #141d27;
}

.shopCart .content .content-left {
  flex: 1;
}

.shopCart .content .content-left .logo-wrapper {
  display: inline-block;
  position: relative;
  top: -46rpx;
  margin: 0 12rpx;
  margin-left: 40rpx;
  width: 99rpx;
  height: 99rpx;
  box-sizing: border-box;
  vertical-align: top;
  border-radius: 50%;
  background: #fff;
  padding: 3rpx;
}

.shopCart .content .content-left .logo-wrapper .num {
  position: absolute;
  top: 5rpx;
  right: -10rpx;
  padding: 0rpx 2rpx;
  min-width: 28rpx;
  height: 33rpx;
  text-align: center;
  border-radius: 16px;
  font-size: 25.3rpx;
  line-height: 33rpx;
  font-weight: 700;
  color: #fff;
  background: rgb(240, 20, 20);
  /* box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.4); */
}

.shopCart .content .content-left .logo-wrapper .logo {
  width: 100%;
  height: 100%;
  text-align: center;
  border-radius: 50%;
  background: #2b343c;
}

.shopCart .content .content-left .logo-wrapper .logo.highlight {
  background: #ff9e04;
}

.shopCart .content .content-left .logo-wrapper .logo.highlight image {
  width: 96rpx;
  height: 96rpx;
}

.shopCart .content .content-left .logo-wrapper .logo .icon-gouwuche {
  line-height: 95rpx;
  font-size: 70rpx;
}

.shopCart .content .content-left .logo-wrapper .logo .icon-gouwuche.highlight {
  color: #fff;
}

.shopCart .content .content-left .pricebox {
  display: inline-block;
  margin-top: 10rpx;
}

.shopCart .content .content-left .price {
  box-sizing: border-box;
  font-size: 30rpx;
  font-weight: 700;
  color: rgba(255, 255, 255, 0.4);
}

.shopCart .content .content-left .price.highlight {
  color: #fff;
}

.shopCart .content .content-left .desc {
  color: #fff;
  font-size: 22rpx;
}

.shopCart .content .content-right {
  flex: 0 0 215rpx;
  width: 215rpx;
}

.shopCart .content .content-right .pay {
  height: 96rpx;
  line-height: 96rpx;
  text-align: center;
  font-size: 32rpx;
  /* //color: rgba(255, 255, 255, 0.4); */
  color: #fff;
  font-weight: 700;
  background: #ccc;
}

.shopCart .content .content-right .pay.not-enough {
  background: #2b333b;
}

.shopCart .content .content-right .pay.enough {
  background: #ff861a;
  color: #fff;
}

.shopCart .ball-container .ball {
  position: fixed;
  left: 32px;
  bottom: 22px;
  z-index: 200;
  transition: all 0.6s cubic-bezier(0.49, -0.29, 0.75, 0.41);
}

.shopCart .ball-container .ball .inner {
  width: 16px;
  height: 16px;
  border-radius: 50%;
  background: rgb(0, 160, 220);
  transition: all 0.4s linear;
}

.shopCart .shopcart-list {
  position: absolute;
  top: 0;
  left: 0;
  z-index: -1;
  width: 100%;
  padding-bottom: 30rpx;
  background: #fff;
  transition: transform 300ms ease 0ms;
  transform-origin: 50% 50% 0px;
  transform: translate(0, 0);
}

.shopCart .shopcart-list.acitve {
  transform: translate(0, auto);
}

.shopCart .shopcart-list :fade-enter-active, :fade-leave-active {
  transition: all 0.5s transform translate3d(0, -100%, 0);
}

.shopCart .shopcart-list :fade-enter, :fade-leave-active {
  transform: translate3d(0, 0, 0);
}

.shopCart .shopcart-list .list-header {
  height: 60rpx;
  line-height: 60rpx;
  padding: 0 20rpx;
  background: #eaeaea;
}

.shopCart .shopcart-list .list-header .title {
  float: left;
  font-size: 28rpx;
  color: #666;
}

.shopCart .shopcart-list .list-header .emptybox {
  float: right;
  font-size: 28rpx;
  color: #666;
}

.shopCart .shopcart-list .list-header .emptybox image {
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
  margin-right: 5rpx;
}

.shopCart .shopcart-list .list-content {
  max-height: 400rpx;
  overflow: hidden;
  background: #fff;
  padding: 10rpx 20rpx;
}

.shopCart .shopcart-list .list-content .shopcart-food {
  position: relative;
  box-sizing: border-box;
  padding: 15rpx 15rpx;
  height: 92rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  /*border-top: 1px solid rgba(7,17,27,0.1);*/
}

.shopCart .shopcart-list .list-content .shopcart-food .name {
  width: 290rpx;
  line-height: 50rpx;
  font-size: 30rpx;
  color: #333;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.shopCart .shopcart-list .list-content .shopcart-food .price {
  line-height: 24px;
  font-size: 36rpx;
  font-weight: 700;
  color: #fd2e2e;
  width: 150rpx;
  margin-right: 30rpx;
}

.shopCart .shopcart-list .list-content .shopcart-food .cartControl-wrapper {
  margin-right: 30rpx;
}

