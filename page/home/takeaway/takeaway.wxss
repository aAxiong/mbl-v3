/* head */

.headers {
  height: 215rpx;
}

.header {
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  color: #fff;
  overflow: hidden;
  background: #000;
  z-index: 48;
  min-height: 215rpx;
}

.header .content-wrapper {
  position: relative;
  /* padding: 24px 12px 18px 24px; */
  font-size: 0;
  min-height: 180rpx;
  padding-bottom: 15rpx;
  animation: slidown 0.1s ease-in both;
}

.header .content-wrapper .content .content-list {
  display: flex;
  width: 630rpx;
  margin: 0 auto;
  color: #fff;
  font-size: 20rpx;
  margin-top: 30rpx;
  flex-flow: row wrap;
  max-height: 84rpx;
  overflow: hidden;
}

.header .content-wrapper .content .hiddenbox.active .content-list {
  max-height: 100%;
  overflow: inherit;
}

.header .content-wrapper .content .hiddenbox.active .downdrap {
  animation: mymove 3s;
}

@keyframes mymove {
  from {
    max-height: 84rpx;
  }

  to {
    max-height: 100%;
  }
}

.header .content-wrapper .content .content-list .discountList {
  width: 50%;
  margin-bottom: 15rpx;
  display: flex;
  align-items: center;
  justify-content: left;
}

.header .content-wrapper .content .content-list .discountList text {
  font-size: 24rpx;
  vertical-align: middle;
}

.header .content-wrapper .content .downdrap {
  text-align: center;
  margin-top: 10rpx;
}

.header .content-wrapper .content .downdrap image {
  width: 36rpx;
  height: 24rpx;
}

.header .content-wrapper .content .content-list image {
  width: 46rpx;
  height: 28rpx;
  margin-right: 15rpx;
}

.header .content-wrapper .local-info {
  color: #fff;
  text-align: center;
  margin: 0 auto;
  padding-top: 20rpx;
  font-size: 30rpx;
}

.header .content-wrapper .local-info image {
  width: 28rpx;
  height: 36rpx;
}

.header .bulletin-wrapper {
  position: relative;
  background-color: rgba(7, 17, 27, 0.2);
  height: 28px;
  line-height: 28px;
  padding: 0 22px 0 12px;
  white-space: normal;
  overflow: hidden;
  text-overflow: ellipsis;
}

.header .background {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: -1;
  filter: blur(1rpx);
  opacity: 0.5;
}

.header .background .detail {
  position: fixed;
  z-index: 100;
  top: 0;
  right: 0;
  width: 100%;
  height: 100%;
  overflow: auto;
  background: rgba(7, 17, 27, 0.8);
  -webkit-backdrop-filter: blur(10px);
}

/* tab */

.tab {
  display: -ms-flexbox;
  display: flex;
  width: 100%;
  height: 40px;
  line-height: 40px;
  /* border: 1px solid rgba(7,17,27,0.1); */
  position: relative;
}

.tab .tab-item {
  -ms-flex: 1;
  flex: 1;
  text-align: center;
}

.tab .tab-item a {
  display: block;
  font-size: 14px;
  color: #4d555d;
}

.tab .tab-item .active {
  color: #f01414;
}

.list-mask {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 40;
  /*backdrop-filter: blur(10px);*/
  -webkit-backdrop-filter: 1;
  background: rgba(7, 17, 27, 0.6);
}

.list-mask :fade-enter-active, :fade-leave-active {
  opacity: 1;
  transition: all 0.5s;
  background: rgba(7, 17, 27, 0.6);
}

.food-list :fade-enter, :fade-leave-active {
  opacity: 0;
  background: rgba(7, 17, 27, 0);
}

